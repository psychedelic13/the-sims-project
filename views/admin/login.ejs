<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title><%= title %></title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="/star-admin-template/vendors/iconfonts/mdi/css/materialdesignicons.min.css">
  <link rel="stylesheet" href="/star-admin-template/vendors/iconfonts/puse-icons-feather/feather.css">
  <link rel="stylesheet" href="/star-admin-template/vendors/css/vendor.bundle.base.css">
  <link rel="stylesheet" href="/star-admin-template/vendors/css/vendor.bundle.addons.css">
  <!-- endinject -->
  <!-- plugin css for this page -->
  <link rel="stylesheet" href="/star-admin-template/stylesheets/css/customstyle.css">
  <!-- End plugin css for this page -->
  <!-- inject:css -->
  <link rel="stylesheet" href="/star-admin-template/stylesheets/css/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="/star-admin-template/images/favicon.png" />
</head>

<body>
  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper auth-page">
      <div class="content-wrapper d-flex align-items-center auth auth-bg-1 theme-one">
        <div class="row w-100">
          <div class="col-lg-4 mx-auto">
            <div class="auto-form-wrapper">
              <form action="/admin/login" method="post">
                <div class="form-group">
                  <label class="label">Username</label>
                  <div class="input-group">
                    <input type="text" class="form-control" placeholder="Username" name="username" value="<%= username %>">
                    <div class="input-group-append">
                        <span class="input-group-text">
                        </span>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <label class="label">Password</label>
                  <div class="input-group">
                    <input type="password" class="form-control" placeholder="Password" name="password">
                    <div class="input-group-append">
                        <span class="input-group-text">
                        </span>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <button class="btn btn-primary submit-btn btn-block">Login</button>
                </div>
              </form>
            </div>
            <br>
            <p class="footer-text text-center">Â© 2018 Bootstrapdash. All rights reserved.</p>
          </div>
        </div>
      </div>
      <!-- content-wrapper ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <!-- plugins:js -->
  <script src="/star-admin-template/vendors/js/vendor.bundle.base.js"></script>
  <script src="/star-admin-template/vendors/js/vendor.bundle.addons.js"></script>
  <!-- endinject -->
  <!-- inject:js -->
  <script src="/star-admin-template/js/off-canvas.js"></script>
  <script src="/star-admin-template/js/misc.js"></script>
  <script src="/star-admin-template/js/hoverable-collapse.js"></script>
  <script src="/star-admin-template/js/settings.js"></script>
  <script src="/star-admin-template/js/todolist.js"></script>
  <!-- endinject -->
</body>

</html>
